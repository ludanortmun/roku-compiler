{
    "categories": [{
        "token": "identifier",
        "regex": "(_|[A-Za-z])(_|[A-Za-z]|[0-9])*",
        "action": "check_value",
        "is_unique": false
    }, {
        "token": "line_end",
        "regex": ";",
        "is_unique": true
    }, {
        "token": "op_arit",
        "regex": "\\+|\\-|\\*|\\/",
        "is_unique": false
    }, {
        "token": "op_not",
        "regex": "!",
        "is_unique": true
    }, {
        "token": "op_log",
        "regex": "\\|\\||&&",
        "is_unique": false
    }, {
        "token": "op_comp",
        "regex": "==|!=|<=|>=|>|<",
        "is_unique": false
    }, {
        "token": "asig",
        "regex": "=",
        "is_unique": true
    }, {
        "token": "zhuli",
        "regex": "-?[0-9]+",
        "is_unique": false
    }, {
        "token": "appa",
        "regex": "-?[0-9]+\\.[0-9]+",
        "is_unique": false
    }, {
        "token": "arg_start",
        "regex": "\\(",
        "is_unique": true
    }, {
        "token": "arg_end",
        "regex": "\\)",
        "is_unique": true
    }, {
        "token": "block_start",
        "regex": "{",
        "is_unique": true
    }, {
        "token": "block_end",
        "regex": "}",
        "is_unique": true
    }, {
        "token": "scroll",
        "regex": "\".*\"",
        "is_unique": false
    }, {
        "token": "wan",
        "regex": "'.'",
        "is_unique": false
    }, {
        "token": "incomplete_appa",
        "regex": "-?[0-9]+\\.",
        "is_unique": false
    }, {
        "token": "incomplete_scroll_wan",
        "regex": "\"[^\"]*|'.",
        "is_unique": false
    }, {
        "token": "scroll_wan_delimiter",
        "regex": "\"|'",
        "is_unique": false
    },{
        "token": "incomplete_op_log",
        "regex": "\\||&",
        "is_unique": false
    }],

    "language_words": [{
        "token": "reserved",
        "regex": "agni|kai|avatar|raava|cabbage|yipyip|energy|bend",
        "is_unique": false
    }, {
        "token": "type",
        "regex": "zhuli|appa|koi|scroll|wan",
        "is_unique": false
    }, {
        "token": "koi",
        "regex": "tui|la",
        "is_unique": false
    }, {
        "token": "null",
        "regex": "sokka",
        "is_unique": true
    }],

    "spaces": {
        "regex": "\\s"
    }
}
